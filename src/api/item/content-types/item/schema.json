{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "shortDescription": {
      "type": "richtext"
    },
    "longDescription": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "sealife",
        "birds",
        "nature",
        "PFM",
        "barrelLids"
      ]
    },
    "colour": {
      "type": "string"
    },
    "productType": {
      "type": "string"
    },
    "featured": {
      "type": "boolean"
    },
    "size": {
      "type": "customField",
      "options": [
        "Small",
        "Medium",
        "Large"
      ],
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
